{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "description": "Nodejs development environment boilerplate",
  "scripts": {
    "start": "npm-run-all --parallel open:src",
    "open:src": "nodemon --exec npm run babel-node -- ./app.js",
    "babel-node": "babel-node --presets=env",
    "security-check": "nsp check",
    "lint": "esw server --color",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel"
  },
  "author": "BSS Dev",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^2.0.1",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express-session": "^1.15.6",
    "express-status-monitor": "^1.1.5",
    "express-validator": "^5.3.0",
    "firebase-admin": "^6.0.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "mongoose": "^5.2.9",
    "morgan": "^1.9.0",
    "nodejs-nodemailer-outlook": "^1.0.8",
    "nodemailer": "^4.6.8",
    "nodemailer-express-handlebars": "^3.0.0",
    "npm-run-all": "^4.1.3",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth": "^1.0.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-linkedin": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "swagger-express": "^1.0.5",
    "twilio": "^3.19.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.4.0",
    "eslint-watch": "^4.0.2",
    "express": "^4.16.3",
    "localtunnel": "^1.9.0",
    "nodemon": "^1.18.3",
    "nsp": "^3.2.1"
  }
}
